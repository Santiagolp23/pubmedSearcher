{
    "manifest_version" : 3,
    "name" : "Pubmed formatted searcher",
    "version" : "1.1",
    "description" : "Search people on PubMed with formatted search, open Email Hunter after the page loads to capture emails",
    "icons" : {
        "128" : "img/icon128.png",
        "48" : "img/icon48.png",
        "16" : "img/icon16.png"
    },
    "action" : {
        "default_icon" : "img/icon48.png",
        "default_popup" : "popup.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}